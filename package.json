{
  "name": "intel-jasmine-n-matchers",
  "version": "2.1.0",
  "description": "A set of matchers to assert a spy was called n times",
  "main": "jasmine-n-matchers.js",
  "repository": {
    "type": "git",
    "url": "https://github.intel.com/IML/intel-jasmine-n-matchers.git"
  },
  "publishConfig": {
    "registry": "https://ubit-artifactory-or.intel.com/artifactory/api/npm/iml-npm-local"
  },
  "scripts": {
    "test": "JASMINE_CONFIG_PATH=jasmine.json NODE_ENV=test jasmine",
    "jshint": "jshint --c ./.jshintrc --exclude-path ./.jshintignore .",
    "jscs": "jscs ."
  },
  "pre-commit": [
    "jscs",
    "jshint",
    "test"
  ],
  "license": "MIT",
  "devDependencies": {
    "jasmine": "2.3.2",
    "jscs": "2.1.0",
    "jshint": "2.8.0",
    "krusty-jasmine-reporter": "1.0.0",
    "pre-commit": "1.1.1"
  },
  "whitelist": [
    "jasmine",
    "krusty-jasmine-reporter"
  ]
}
